package trabalhandoComStrings;

public class exemplo1 {

	public static void main(String[] args) {
		
		String s = "Ana";
        
        for(int i = 0; i < s.length(); i++) {
        
            int cont = 0;
                    
            for(int j = 0; j < s.length(); j++) {
                
              if(s.charAt(i) == s.charAt(j))
                  cont++;
            }
            
            System.out.println(s.charAt(i) + " = " + cont);
            cont = 0;
        }
	}

}
